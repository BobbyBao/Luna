class Tuple {
    
    init(...) {
        self.items = {...}
    }

    func == (t1, t2)  {

        if #t1 != #t2 {
            return false
        }

        for k,v in ipairs(t1) {
            if v != t2[k] {
                return false
            }
        }

        return true
    }
}
